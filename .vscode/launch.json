    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/Nodepop",
            "program": "${workspaceFolder}/Nodepop/server.js",
            "env": {
                "DEBUG": "Nodepop:*",
                "NODEPOP_ENV":"development",
                "PORT": "3000",
            },
            "outputCapture": "std"
        },
        
    ]
}